generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") //Conexion a la base de datos, en el archivo .env
}
//Modelo de ejemplo
model User {
  id    String @id @default(uuid()) @db.Uuid
  name  String
  email String @unique
}
